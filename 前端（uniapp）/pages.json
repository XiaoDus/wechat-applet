{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
				// "enablePullDownRefresh": true


			}
		},
		{
			"path": "pages/fn/fn",
			"style": {
				"navigationBarTitleText": "任务"

			}
		},
		{
			"path": "pages/fn/addTask/addTask",
			"style": {
				"navigationBarTitleText": "添加任务"
			}
		},

		{
			"path": "pages/store/store",
			"style": {
				"navigationBarTitleText": "兑换"
			}
		},


		{
			"path": "pages/user/user",
			"style": {
				"navigationBarTitleText": "卡片"
			}
		},
		
		{
			"path": "pages/fn/taskFinish/taskFinish",
			"style": {
				"navigationBarTitleText": "任务详情",
				"enablePullDownRefresh": false
			}

		}
	    ,{
            "path" : "pages/user/userInfo/userInfo",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "个人信息",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"condition": { //模式配置，仅开发期间生效
		"current": 0, //当前激活的模式（list 的索引项）
		"list": [{
				"name": "首页", //模式名称
				"path": "pages/index/index" //启动页面，必选
			},
			{
				"name": "任务", //模式名称
				"path": "pages/fn/fn" //启动页面，必选
			},
			{
				"name": "兑换", //模式名称
				"path": "pages/store/store" //启动页面，必选
			},
		

			{
				"name": "卡片", //模式名称
				"path": "pages/user/user" //启动页面，必选
			},
			{
				"name": "个人信息", //模式名称
				"path": "pages/user/userInfo/userInfo" //启动页面，必选
			},
			
			{
				"name": "添加任务", //模式名称
				"path": "pages/fn/addTask/addTask" //启动页面，必选
			},
			{
				"name": "任务详情", //模式名称
				"path": "pages/fn/taskFinish/taskFinish" //启动页面，必选
			}
		]
	},

	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#f6f6f6"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3cc51f",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/index/index",
				"iconPath": "static/TabBar/index.jpg",
				"selectedIconPath": "static/TabBar/indexSelect.jpg",
				"text": "首页"
			}, {
				"pagePath": "pages/fn/fn",
				"iconPath": "static/TabBar/task.jpg",
				"selectedIconPath": "static/TabBar/taskSelect.jpg",
				"text": "任务"
			},
			{
				"pagePath": "pages/store/store",
				"iconPath": "static/TabBar/store.jpg",
				"selectedIconPath": "static/TabBar/storeSelect.jpg",
				"text": "兑换"
			},

			{
				"pagePath": "pages/user/user",
				"iconPath": "static/TabBar/roll.jpg",
				"selectedIconPath": "static/TabBar/rollSelect.jpg",
				"text": "卡片"
			}
		]
	},
	"uniIdRouter": {},
	"name": "uni-sms",
	"extensions": {
		"uni-cloud-sms": {} // 启用uni-cloud-sms扩展，值为空对象即可
	}
}
